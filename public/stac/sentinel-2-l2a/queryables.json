{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "https://example.org/queryables",
  "type": "object",
  "title": "Queryables for mock PC Sentinel-2-l2a STAC API",
  "description": "Queryable properties for the mock STAC API Item Search filter.",
  "properties": {
    "id": {
      "description": "ID",
      "$ref": "https://schemas.stacspec.org/v1.0.0/item-spec/json-schema/item.json#/id"
    },
    "geometry": {
      "description": "Geometry",
      "$ref": "https://schemas.stacspec.org/v1.0.0/item-spec/json-schema/item.json#/geometry"
    },
    "datetime": {
      "description": "Datetime",
      "title": "Acquired",
      "format": "date-time",
      "pattern": "(\\+00:00|Z)$"
    },
    "eo:cloud_cover": {
      "$ref": "https://stac-extensions.github.io/eo/v1.0.0/schema.json#/definitions/fields/properties/eo:cloud_cover"
    },
    "s2:snow_ice_percentage": {
      "title": "Snow/Ice Cover",
      "type": "number",
      "min": 0,
      "max": 100
    },
    "sat:orbit_state": {
      "$ref": "https://stac-extensions.github.io/sat/v1.0.0/schema.json#/definitions/fields/properties/sat:orbit_state"
    }
  }
}
